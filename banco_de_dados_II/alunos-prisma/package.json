    "@types/supertest": "^6.0.2",
    "@types/swagger-ui-express": "^4.1.8",
    "prisma": "^6.0.1",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.6",
    "ts-node-dev": "^2.0.0",
    "typedoc": "^0.27.9",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "@prisma/client": "^6.0.1",
